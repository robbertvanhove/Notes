<Window x:Class="IntroWPF.WindowCanvas"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IntroWPF"
        mc:Ignorable="d"
        Title="WindowCanvas" Height="300" Width="300">
    <Grid>
        <Canvas>
            <Ellipse Fill="Green" Width="60" Height="60" Canvas.Left="30" 	Canvas.Top="20" 	Canvas.ZIndex="1"/>
            <Ellipse Fill="Blue"  Width="60" Height="60" Canvas.Left="60" 	Canvas.Top="40"/>
        </Canvas>

    </Grid>
</Window>
